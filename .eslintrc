{
    "rules": {
        "indent": [
            2,
            2
        ],
        "quotes": [
            2,
            "single"
        ],
        "linebreak-style": [
            2,
            "unix"
        ],
        "semi": [
            2,
            "always"
        ],
        "max-len": [
          2,
          80,
          4
        ]
    },
    "env": {
        "node": true
    },
    "extends": "eslint:recommended"
}
